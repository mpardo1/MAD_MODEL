# Moving average function:
mov_avg <- function(dim, dt, vec_in){
vec_out = c(1:dim)
for(i in c(1:dim)){
if( i < (dim - dt)){
end = i + dt
vec_out[i] = mean(vec_in[i:end])
}else{
vec_out[i] = mean(vec_in[i:dim])
}
}
return(vec_out)
}
