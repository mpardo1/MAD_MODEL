# }
# Larva mortality rate
delta_L <- function(T){
val = 0.021643*T^2 - 0.959568*T + 10.440131
val = max(0, val)
return(val)
}
# Adult mosquito mortality rate
delta_A <- function(T){
a = abs(-0.1921*T^2+8.147*T-22.98)
val =  min(1, 1/a)
return(val)
}
