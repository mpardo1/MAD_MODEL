# Functional responses:
# Gonotrophic cycle (Carlos):
gonot <- function(T){
a = 0.045*T^2-2.617*T+41.105
val = min(1,1/a)
return(val)
}
# Development rate
# d_L <- function(T){
#   val = 1/(0.14457*T^2 - 8.24857*T + 124.80857)
#   return(val)
# }
# Carlos version:
d_L <- function(T){
val = (0.14457*Tˆ2 - 8.24857*T + 124.80857)ˆ(-1)
return(val)
}
